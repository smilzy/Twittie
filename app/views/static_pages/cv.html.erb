                        <div class="clearfix">
                            <div class="col-xs-4">
                                <div class="name"> Angielski </div>
                            </div>
                            <div class="col-xs-8">
                                <div class="rating">
                                    <% stars = "<i class='fa fa-star fa-fw'></i>" *5 %>
                                    <%= stars.html_safe %>
                                </div>
                            </div>
                        </div>
                        
                        <div class="clearfix">
                            <div class="col-xs-4">
                                <div class="name"> Niemiecki </div>
                            </div>
                            <div class="col-xs-8">
                                <div class="rating">
                                    <% stars = "<i class='fa fa-star fa-fw'></i>" *1 %>
                                    <% stars_blank = "<i class='fa fa-star fa-fw transparent'></i>" *3 %>
                                    <% star_half = "<i class='fa fa-star-half-o fa-fw'></i>" %>
                                    <%= stars.html_safe + star_half.html_safe + stars_blank.html_safe %>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
